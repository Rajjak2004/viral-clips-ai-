# ViralClips AI Environment Variables

# Klap API Configuration
REACT_APP_KLAP_API_BASE_URL=https://api.klap.app/v2
REACT_APP_KLAP_API_KEY=your_klap_api_key_here

# App Configuration
REACT_APP_APP_NAME=ViralClips AI
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=AI-powered video editor that turns long videos into viral shorts

# GitHub Pages Deployment
PUBLIC_URL=https://yourusername.github.io/viral-clips-ai

# Analytics (Optional)
REACT_APP_GA_TRACKING_ID=G-XXXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token_here

# Feature Flags
REACT_APP_ENABLE_MOCK_API=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=false

# API Rate Limiting
REACT_APP_MAX_FILE_SIZE=2147483648  # 2GB in bytes
REACT_APP_MAX_CLIPS_PER_VIDEO=10
REACT_APP_MAX_VIDEO_DURATION=3600   # 1 hour in seconds

# Social Media Links
REACT_APP_GITHUB_URL=https://github.com/yourusername/viral-clips-ai
REACT_APP_TWITTER_URL=https://twitter.com/yourusername
REACT_APP_DISCORD_URL=https://discord.gg/viral-clips-ai

# Contact Information
REACT_APP_SUPPORT_EMAIL=support@viralclips.ai
REACT_APP_CONTACT_EMAIL=hello@viralclips.ai
